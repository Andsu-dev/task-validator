{
  "name": "task-validator-agent",
  "version": "1.1.0",
  "description": "Agent para validação de tasks usando regras de negócio",
  "main": "dist/index.js",
  "bin": {
    "task-validator": "dist/cli.js"
  },
  "preferGlobal": true,
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "cli": "tsx src/cli-simple.ts",
    "cli:build": "tsc && node dist/cli-simple.js",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts"
  },
  "keywords": [
    "ai",
    "agent",
    "validation",
    "git",
    "task"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.0.0",
    "dotenv": "^16.3.1",
    "simple-git": "^3.19.1",
    "fs-extra": "^11.1.1",
    "path": "^0.12.7",
    "winston": "^3.10.0",
    "joi": "^17.9.2",
    "multer": "^1.4.5-lts.1",
    "@langchain/core": "^0.1.0",
    "@langchain/google-genai": "^0.0.8",
    "@langchain/community": "^0.0.20",
    "langchain": "^0.1.0",
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "inquirer": "^8.2.6"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/cors": "^2.8.13",
    "@types/fs-extra": "^11.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "eslint": "^8.46.0",
    "jest": "^29.6.2",
    "@types/jest": "^29.5.3",
    "ts-jest": "^29.1.1",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "prettier": "^3.0.0",
    "@types/inquirer": "^8.2.10"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
